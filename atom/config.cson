"*":
  "autocomplete-plus":
    confirmCompletion: "tab"
  autosave:
    enabled: true
  core:
    disabledPackages: [
      "language-xml"
      "language-typescript"
      "language-sql"
      "language-rust-bundled"
      "language-ruby-on-rails"
      "language-ruby"
      "language-property-list"
      "language-php"
      "language-perl"
      "language-objective-c"
      "language-less"
      "language-css"
      "language-csharp"
      "language-clojure"
      "language-c"
      "autocomplete-css"
      "bookmarks"
      "deprecation-cop"
      "language-coffee-script"
      "language-html"
      "metrics"
      "timecop"
      "welcome"
      "exception-reporting"
      "markdown-preview"
    ]
    packagesWithKeymapsDisabled: []
    telemetryConsent: "no"
  editor:
    autoIndentOnPaste: false
    fontSize: 18
    invisibles: {}
    normalizeIndentOnPaste: false
    showInvisibles: true
    softWrap: true
  "find-and-replace":
    enablePCRE2: true
    useRipgrep: true
  "fuzzy-finder":
    scoringSystem: "fast"
  "git-diff":
    showIconsInEditorGutter: true
  hyperclick:
    darwinTriggerKeys: "ctrlKey"
  linter: {}
  "linter-js-yaml":
    customTags: [
      "!vault"
    ]
  "linter-pylint":
    disableTimeout: true
    pythonPath: "/usr/local/bin/python3"
    rcFile: "~/.pylintrc"
  "linter-shellcheck":
    enableNotice: true
  "linter-ui-default":
    panelHeight: 206
  "markdown-preview":
    useGitHubStyle: true
  "markdown-preview-plus":
    saveConfig:
      saveToPDFOptions: {}
    style: "github"
    syncConfig:
      syncEditorOnPreviewScroll: true
      syncPreviewOnChange: true
      syncPreviewOnEditorScroll: true
  minimap:
    displayCodeHighlights: false
    plugins:
      cursorline: true
      cursorlineDecorationsZIndex: 0
      "find-and-replace": true
      "find-and-replaceDecorationsZIndex": 0
      "git-diff": true
      "git-diffDecorationsZIndex": 0
      "highlight-selected": true
      "highlight-selectedDecorationsZIndex": 0
      linter: true
      linterDecorationsZIndex: 0
      "minimap-autohide": true
      "minimap-autohideDecorationsZIndex": 0
      pigments: true
      pigmentsDecorationsZIndex: 0
      selection: true
      selectionDecorationsZIndex: 0
  "spell-check": {}
  terminus:
    core:
      workingDirectory: "Active File"
    toggles:
      loginShell: false
      runInsertedText: false
  "tree-view":
    alwaysOpenExisting: true
  welcome:
    showOnStartup: false
  whitespace:
    ignoreWhitespaceOnCurrentLine: false
".html.mustache.text":
  editor:
    autoIndent: true
    autoIndentOnPaste: true
    showIndentGuide: true
".python.source":
  editor:
    showIndentGuide: true
